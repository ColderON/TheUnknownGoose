<Page x:Class="TheUnknownGoose.Page_Add"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TheUnknownGoose"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Add" Loaded="Page_Loaded">  
    

    
    <Grid Background="AliceBlue">        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"/>
            <ColumnDefinition Width="300"/>
        </Grid.ColumnDefinitions>

        <TreeView x:Name="mainTreeView" DockPanel.Dock="Top" Grid.Column="0" Margin="10" SelectedItemChanged="mainTreeView_SelectedItemChanged"/>
        <Label Name="label" Grid.Column="1" Width="400" Height="100"/>
    </Grid>
</Page>
